# Baileys WhatsApp Service Configuration
PORT=3100
HOST=127.0.0.1

# Webhook URL (PHP app endpoint)
WEBHOOK_URL=http://127.0.0.1/chat/whatsapp-webhook

# Auth token for API security
API_TOKEN=change_this_to_a_secure_random_token

# Log level: fatal, error, warn, info, debug, trace
LOG_LEVEL=info

# Session storage path
SESSIONS_PATH=./src/store
